/* cmakeconfig.h.in */

#ifndef __CONFIG_H__
#define __CONFIG_H__

/* Note: You cannot use earlier #defines in later #cmakedefines (cmake 2.6.2). */

/* Name of package */
#define PACKAGE "Swiftray"

/* Version number of Wireshark and associated utilities */
#define PROJECT_VERSION_STR "${PROJECT_VERSION_STR}"
#define VERSION_MAJOR ${VERSION_MAJOR}
#define VERSION_MINOR ${VERSION_MINOR}
#define VERSION_BUILD ${VERSION_BUILD}
#define VERSION_BETA  ${VERSION_BETA}
#define VERSION_SUFFIX "${VERSION_SUFFIX}"

/* Define to 1 if you have the Sparkle or WinSparkle library */
#cmakedefine HAVE_SOFTWARE_UPDATE 1

#endif /* __CONFIG_H__ */
